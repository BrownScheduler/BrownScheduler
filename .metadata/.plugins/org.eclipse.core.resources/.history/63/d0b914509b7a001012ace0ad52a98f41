package roundrobin2;

public class Round extends backbone.Round {

	private Turn _t;
	private int _roundNum;
	public Round(Turn t, String name) {
		super(name);
		_t = t;
	}
	
	public void addGame(Game g){
		this.pairings.add(g);
	}
	
	@Override
	public Game getBlank(){
		return new Game(_t, this, "");
	}

}
